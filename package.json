{
    "name": "mixnode",
    "version": "0.1.0",
    "description": "Unofficial Mixcloud deskop application made with Electron & React.js",
    "main": "mixnode.js",
    "license": "MIT",
    "scripts": {
        "precommit": "npm test",
        "start": "electron ./src/mixnode",
        "start-dev": "NODE_ENV=development electron ./src/index",
        "build-main": "webpack --config ./config/webpack.electron.js",
        "build-client": "rimraf ./src/dist && webpack --config ./config/webpack.prod.js --progress --profile --colors",
        "build": "npm run build-main && npm run build-client",
        "package": "npm run build && build --linux",
        "server": "node ./server",
        "dev": "npm run server -- --hot",
        "test": "jest --coverage",
        "test:watch": "jest --watch"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/app/**/*.js"
        ],
        "testMatch": [
            "**/test/**/*.spec.js?(x)"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mocks/fileMock.js",
            "\\.(css|less|sass|scss)$": "<rootDir>/test/mocks/styleMock.js"
        },
        "moduleFileExtensions": [
            "js"
        ],
        "moduleDirectories": [
            "node_modules",
            "src/node_modules"
        ],
        "transform": {
            "^.+\\.js$": "babel-jest"
        }
    },
    "build": {
        "productName": "Mixnode",
        "appId": "org.develar.Mixnode",
        "files": [
            "dist/",
            "mixnode.js",
            "package.json"
        ],
        "linux": {
            "target": [
                "AppImage"
            ]
        },
        "directories": {
            "app": "src",
            "buildResources": "resources",
            "output": "release"
        }
    },
    "bin": {
        "electron": "./node_modules/.bin/electron"
    },
    "keywords": [
        "mixnode",
        "mixcloud",
        "music",
        "electron",
        "react",
        "redux",
        "player"
    ],
    "author": {
        "name": "Ivan Mrvelj",
        "email": "sk8raid@gmail.com",
        "url": "https://github.com/imrvelj"
    },
    "devDependencies": {
        "babel-core": "^6.24.1",
        "babel-eslint": "^7.2.3",
        "babel-jest": "^19.0.0",
        "babel-loader": "^7.0.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "babel-register": "^6.24.1",
        "css-loader": "^0.28.1",
        "devtron": "^1.4.0",
        "electron": "^1.6.6",
        "electron-builder": "^17.1.2",
        "enzyme": "^2.8.2",
        "eslint": "^3.19.0",
        "eslint-loader": "^1.7.1",
        "eslint-plugin-jest": "^19.0.1",
        "eslint-plugin-react": "^6.10.2",
        "file-loader": "^0.11.1",
        "html-webpack-plugin": "^2.28.0",
        "ignore-styles": "^5.0.1",
        "jest": "^19.0.2",
        "json-loader": "^0.5.4",
        "minimist": "^1.2.0",
        "node-sass": "^4.5.2",
        "react-addons-test-utils": "^15.5.1",
        "redux-mock-store": "^1.2.3",
        "rimraf": "^2.6.1",
        "sass-loader": "^6.0.3",
        "sinon": "^2.2.0",
        "source-map-loader": "^0.2.1",
        "style-loader": "^0.17.0",
        "webpack": "^2.4.1",
        "webpack-dev-server": "^2.4.5",
        "webpack-dotenv-plugin": "^2.0.0",
        "webpack-merge": "^4.1.0"
    },
    "dependencies": {
        "asar": "^0.13.0",
        "axios": "^0.16.1",
        "dotenv": "^4.0.0",
        "history": "^4.6.1",
        "lodash": "^4.17.4",
        "moment": "^2.18.1",
        "react": "^15.5.4",
        "react-dom": "^15.5.4",
        "react-redux": "^5.0.4",
        "react-router": "^4.1.1",
        "react-router-redux": "next",
        "redux": "^3.6.0",
        "redux-thunk": "^2.1.0",
        "webpack-dev-middleware": "^1.10.2",
        "webpack-hot-middleware": "^2.18.0"
    }
}
